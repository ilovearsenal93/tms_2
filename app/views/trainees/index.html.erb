<% provide(:title, 'All trainees') %>
<h1>All trainees</h1>
<%= will_paginate %>
<ul class="trainees">
  <% @trainees.each do |trainee| %>
    <% unless supervisor? trainee %>
      <li>
      <%= gravatar_for trainee, size: 52 %>
      <%= link_to trainee.name, trainee %>
      </li>
    <% else %>
    <% end %>  
  <% end %>
</ul>

<%= will_paginate %>
